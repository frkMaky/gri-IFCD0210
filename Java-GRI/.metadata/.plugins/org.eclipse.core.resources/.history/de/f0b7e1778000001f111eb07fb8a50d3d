
public class EjemploContinue {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for (int i = 0; i < 10; i++) {
			if (i == 4) {
				continue;
			}
			System.out.println(i);
		}
		
		// Escribe los multiplos de 5 de 20 a 50. En 40 no imprimes
		int num = 20;
		while (num <= 50) {
			if ( num == 40 ) {
				continue;
			}
			System.out.println(num);
			num +=5;
		}
	}

}
